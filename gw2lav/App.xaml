<Application x:Class="gw2lav.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:gw2lav"
	StartupUri="View/MainWindow.xaml">
	
	<Application.Resources>
		<ResourceDictionary>
			
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="View/Style/Fonts.xaml" />
				<ResourceDictionary Source="View/Style/Windows.xaml" />
				<ResourceDictionary Source="View/Style/Buttons.xaml" />
			</ResourceDictionary.MergedDictionaries>
			
			<local:ViewModelLocator x:Key="VMLocator" />
			
		</ResourceDictionary>
	</Application.Resources>
	
</Application>
